{
  "name": "origin-squid",
  "private": true,
  "scripts": {
    "codegen": "echo '# GENERATED, DO NOT MODIFY\n' > schema.graphql && cat $(find src -name '*.graphql') >> schema.graphql && sqd codegen && git add src/model/generated/*",
    "migration:generate": "sqd down && sqd up && sqd migration:generate && git add db/migrations/*",
    "generate": "npm-run-all codegen migration:generate",
    "generate:validations": "ts-node --require tsconfig-paths/register scripts/generate-validations.ts",
    "build": "rm -rf lib && tsc",
    "prettier-check": "prettier --check src",
    "prettier-fix": "prettier --write src",
    "setup": "sqd down && sqd up && sleep 2 && sqd migration:apply",
    "serve": "sqd serve",
    "process:arbitrum": "sqd process:arbitrum",
    "process:base": "sqd process:base",
    "process:sonic": "sqd process:sonic",
    "process:oeth": "sqd process:oeth",
    "process:ousd": "sqd process:ousd",
    "process:ogv": "sqd process:ogv",
    "process:mainnet": "sqd process:mainnet",
    "process:test": "sqd process:test",
    "process": "sqd process",
    "log:processing-times": "ts-node --require tsconfig-paths/register scripts/check-processing-times.ts"
  },
  "dependencies": {
    "@subsquid/evm-processor": "^1.26.1",
    "@subsquid/graphql-server": "^4.9.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "@tanstack/query-core": "^5.32.0",
    "@types/lodash": "^4.14.200",
    "dayjs": "^1.11.10",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^16.1.4",
    "esbuild": "^0.19.5",
    "ethers": "^6.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.14",
    "type-graphql": "^1.2.0-rc.1",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0",
    "viem": "^1.21.4"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^4.3.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^18.16.17",
    "@types/node-fetch": "^2.6.9",
    "@types/uuid": "^9.0.2",
    "@types/websocket": "^1.0.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.1.3"
  }
}
