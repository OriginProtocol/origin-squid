{
  "name": "origin-squid",
  "private": true,
  "scripts": {
    "codegen": "echo '# GENERATED, DO NOT MODIFY\n' > schema.graphql && cat $(find src -name '*.graphql') >> schema.graphql && sqd codegen && git add src/model/generated/*",
    "migration:generate": "sqd down && sqd up && sqd migration:generate && git add db/migrations/*",
    "generate": "npm-run-all codegen migration:generate",
    "build": "rm -rf lib && tsc",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register 'src/**/*.test.ts'",
    "test:watch": "mocha --require ts-node/register --watch 'src/**/*.test.ts'",
    "prettier-check": "prettier --check src",
    "prettier-fix": "prettier --write src",
    "setup": "sqd down && sqd up && sleep 2 && sqd migration:apply",
    "serve": "sqd serve",
    "typegen": "sqd typegen",
    "migration:apply": "sqd migration:apply",
    "process:arbitrum": "sqd process:arbitrum",
    "process:base": "sqd process:base",
    "process:sonic": "sqd process:sonic",
    "process:os": "sqd process:os",
    "process:oeth": "sqd process:oeth",
    "process:oethb": "sqd process:oethb",
    "process:ousd": "sqd process:ousd",
    "process:ogv": "sqd process:ogv",
    "process:plume": "sqd process:plume",
    "process:mainnet": "sqd process:mainnet",
    "process:test": "sqd process:test",
    "process": "sqd process",
    "generate:validations": "ts-node --require tsconfig-paths/register scripts/generate-validations.ts",
    "log:processing-times": "ts-node --require tsconfig-paths/register scripts/check-processing-times.ts",
    "postdeploy": "sh -c 'npm run log:processing-times $1 && npm run generate:validations $1' --",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.808.0",
    "@lavamoat/preinstall-always-fail": "^2.1.1",
    "@originprotocol/squid-utils": "github:OriginProtocol/squid-utils#main",
    "@subsquid/evm-processor": "^1.27.2",
    "@subsquid/graphql-server": "^4.11.0",
    "@subsquid/typeorm-migration": "^1.3.0",
    "@subsquid/typeorm-store": "^1.5.1",
    "@tanstack/query-core": "^5.32.0",
    "@types/lodash": "^4.14.200",
    "chai": "^4.3.10",
    "dayjs": "^1.11.10",
    "deep-object-diff": "^1.1.9",
    "dotenv": "^16.1.4",
    "esbuild": "^0.19.5",
    "ethers": "^6.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.0",
    "pg-copy-streams": "^6.0.6",
    "reflect-metadata": "^0.1.14",
    "type-graphql": "^1.2.0-rc.1",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0",
    "viem": "^2.22.12"
  },
  "devDependencies": {
    "@lavamoat/allow-scripts": "^3.3.4",
    "@subsquid/evm-typegen": "^4.4.0",
    "@subsquid/typeorm-codegen": "^2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/chai": "^4.3.11",
    "@types/js-yaml": "^4.0.8",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.16.17",
    "@types/node-fetch": "^2.6.9",
    "@types/pg": "^8.15.1",
    "@types/pg-copy-streams": "^1.2.5",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^9.0.2",
    "@types/websocket": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.1.3"
  },
  "lavamoat": {
    "allowScripts": {
      "@lavamoat/preinstall-always-fail": false,
      "@subsquid/evm-processor>@subsquid/rpc-client>websocket>es5-ext": false,
      "@subsquid/evm-typegen>@subsquid/evm-abi>keccak256>keccak": false,
      "@subsquid/graphql-server>@subsquid/apollo-server-core>@apollo/utils.usagereporting>@apollo/usage-reporting-protobuf>@apollo/protobufjs": false,
      "@subsquid/graphql-server>@subsquid/apollo-server-core>apollo-reporting-protobuf>@apollo/protobufjs": false,
      "esbuild": false,
      "pg>pg-native>libpq": false,
      "type-graphql": false,
      "viem>ws>bufferutil": false,
      "viem>ws>utf-8-validate": false
    }
  }
}
