{
  "name": "origin-squid",
  "private": true,
  "scripts": {
    "codegen": "echo '# GENERATED, DO NOT MODIFY\n' > schema.graphql && cat schema-*.graphql >> schema.graphql && sqd codegen && git add src/model/generated/*",
    "migration:generate": "sqd down && sqd up && sqd migration:generate",
    "build": "rm -rf lib && tsc",
    "prettier-check": "prettier --check **/*.ts **/*.json",
    "prettier-fix": "prettier --write **/*.ts **/*.json",
    "setup": "sqd down && sqd up && sleep 2 && sqd migration:apply",
    "process:oeth": "sqd process:oeth",
    "process:ousd": "sqd process:ousd",
    "process:ogv": "sqd process:ogv",
    "process:other": "sqd process:other",
    "process:lrt": "sqd process:lrt",
    "process": "npm-run-all --parallel process:oeth process:ousd process:ogv process:other"
  },
  "dependencies": {
    "@subsquid/archive-registry": "^3.3.0",
    "@subsquid/evm-processor": "^1.8.5",
    "@subsquid/graphql-server": "^4.3.1",
    "@subsquid/typeorm-migration": "^1.2.2",
    "@subsquid/typeorm-store": "^1.2.4",
    "@types/lodash": "^4.14.200",
    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "dotenv": "^16.1.4",
    "esbuild": "^0.19.5",
    "ethers": "^6.5.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.14",
    "type-graphql": "^1.2.0-rc.1",
    "typeorm": "^0.3.16",
    "uuid": "^9.0.0",
    "viem": "^1.10.13"
  },
  "devDependencies": {
    "@subsquid/evm-typegen": "^3.2.3",
    "@subsquid/typeorm-codegen": "^1.3.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^18.16.17",
    "@types/node-fetch": "^2.6.9",
    "@types/uuid": "^9.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "typescript": "~5.1.3"
  }
}
